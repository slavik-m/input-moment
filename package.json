{
  "name": "input-moment",
  "version": "0.1.0",
  "description": "React datetime picker powered by momentjs",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server -c --port=8888",
    "build-js": "node_modules/.bin/webpack -p",
    "build-css": "node_modules/.bin/lessc --clean-css src/less/input-moment.less dist/input-moment.css",
    "build": "npm run build-js && npm run build-css",
    "deploy": "npm run build && github-pages-deploy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm dist/* && rm example/bundle*"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "github-pages-deploy": {
    "src": "example"
  },
  "author": "Wang Zuo",
  "license": "ISC",
  "peerDependencies": {
    "react": "^0.15.1",
    "react-dom": "^0.15.1",
    "moment": "^2.10.6"
  },
  "devDependencies": {
    "babel-core": "^6.1.2",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "css-loader": "^0.21.0",
    "github-pages-deploy": "0.0.3",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "less-plugin-clean-css": "^1.5.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.3",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "blacklist": "^1.1.2",
    "classnames": "^2.2.0",
    "lodash": "^3.10.1",
    "moment-timezone": "^0.5.4",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-input-slider": "HonkMobileCo/react-input-slider",
    "react-select": "^1.0.0-beta13"
  },
  "directories": {
    "example": "example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangzuo/input-moment.git"
  },
  "keywords": [
    "react",
    "react-component",
    "input",
    "datetime",
    "picker",
    "moment"
  ],
  "bugs": {
    "url": "https://github.com/wangzuo/input-moment/issues"
  },
  "homepage": "https://github.com/wangzuo/input-moment#readme"
}
